    text-align: center; /* Выравнивание текста по центру */
    text-decoration: none; /* Без подчеркивания текста */
    display: inline-block; /* Для блоков */
    font-size: 20px; /* Размер шрифта */
    margin: 4px 2px; /* Отступы вокруг кнопки */
    cursor: pointer; /* Курсор-указатель при наведении */
    border-radius: 15px; /* Скругление углов */
    transition: background-color 0.3s, box-shadow 0.3s; /* Плавный переход для фона и тени */
    box-shadow: 3px 3px 5px rgba(0, 0, 0, 0.3); /* Добавление тени */
}

QPushButton:hover {
    background-color: #c26161; /* Более темный красный при наведении */
}

QPushButton:pressed {
    background-color: #944b4b; /* Еще более темный красный при нажатии */
    box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2); /* Уменьшение тени при нажатии */
}
</string>
